{
  "name": "frontend-example",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssl": "ng serve --ssl --ssl-key ssl/localhost.key  --ssl-cert ssl/localhost.crt",
    "build": "ng build --base-ref ./",
    "watch": "ng build --watch --configuration development",
    "test": "jest --config jest.config.ts",
    "pact:generate": "jest --config jest.pact.config.ts --runInBand",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky install",
    "cypress": "cypress",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "start-server-and-test start http://localhost:4200 cypress:run",
    "e2e:open": "start-server-and-test start http://localhost:4200 cypress:open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@ngrx/store": "^15.4.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.0",
    "@badeball/cypress-cucumber-preprocessor": "^15.1.5",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@jest/globals": "^29.5.0",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/store-devtools": "^15.4.0",
    "@pact-foundation/pact": "^11.0.2",
    "@pact-foundation/pact-node": "^10.17.7",
    "@types/jasmine": "~4.3.0",
    "@types/jest": "^29.5.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.8.1",
    "esbuild": "^0.17.12",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "jest": "^28.1.3",
    "jest-preset-angular": "^13.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.5",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  }
}
